#!/bin/sh

# List Company Apps
# https://apidocs.apigee.com/management/apis/get/organizations/%7Borg_name%7D/companies/%7Bcompany_name%7D/apps-0

curl -sSf \
  -H "Authorization: Bearer $TOKEN" \
  "https://apidude-eval-test.apigee.net/mgmt-proxy/v1/organizations/$APIGEE_ORG/companies/$APIGEE_COMPANY/apps"
#  "https://api.enterprise.apigee.com/v1/organizations/$APIGEE_ORG/companies/$APIGEE_COMPANY/apps"

